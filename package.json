  "scripts": {
    "postinstall": "bundle",
    "postbuild": "purgecss --css /home/runner/work/wizardunicorn.ninja/wizardunicorn.ninja/assets/css/*.css --content /home/runner/work/wizardunicorn.ninja/wizardunicorn.ninja/*.html --output /home/runner/work/wizardunicorn.ninja/wizardunicorn.ninja/assets/css/new",
    "start": "bundle exec jekyll serve",
    "update-uswds": "npm run update-assets && npm run update-sass",
    "update-assets": "rsync -avr --delete node_modules/uswds/dist/ assets/uswds/ --exclude=/*.md --exclude=/*.zip --exclude=/css --exclude=/scss",
    "update-sass": "rsync -avr --delete node_modules/uswds/src/stylesheets/ _sass/uswds/"
  },
  "dependencies": {
    "acorn": "^7.4.0",
    "purgecss": "^3.0.0",
    "uswds": "^1.6.13"
  },
  "devDependencies": {
    "cached-path-relative": "^1.0.2"
  }
}
