  weapons: [</span><span class="dl">'</span><span class="nx">candlestick</span><span class="dl">'</span><span class="s1">, </span><span class="dl">'</span><span class="nx">dagger</span><span class="dl">'</span><span class="s1">, </span><span class="dl">'</span><span class="nx">lead</span> <span class="nx">pipe</span><span class="dl">'</span><span class="s1">, </span><span class="dl">'</span><span class="nx">revolver</span><span class="dl">'</span><span class="s1">, </span><span class="dl">'</span><span class="nx">rope</span><span class="dl">'</span><span class="s1">, </span><span class="dl">'</span><span class="nx">spanner</span><span class="dl">'</span><span class="s1">],
